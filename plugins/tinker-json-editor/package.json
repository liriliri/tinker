{
  "name": "tinker-json-editor",
  "version": "0.1.0",
  "description": "JSON Editor Plugin for Tinker",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\"",
    "dev:renderer": "vite --config ../vite.config.ts"
  },
  "tinker": {
    "name": "JSON Editor",
    "description": "A simple JSON editor with syntax highlighting and validation",
    "icon": "icon.png",
    "main": "dist/renderer/index.html",
    "preload": "dist/preload/index.js",
    "locales": {
      "zh-CN": {
        "name": "JSON 编辑器",
        "description": "JSON 编辑器，支持语法高亮和校验"
      }
    }
  }
}
